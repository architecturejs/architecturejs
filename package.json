{
  "name": "architecturejs",
  "version": "0.0.0-development",
  "description": "A place for putting the best architectures for any kind of JS project/app ",
  "main": "src/Main.js",
  "scripts": {
    "commit": "git-cz",
    "start": "yarn run dev",
    "dev": "webpack-dev-server --inline --progress",
    "build": "rimraf dist && webpack --config config/webpack.production.js --progress --profile --bail",
    "test": "NODE_ENV=test jest --no-cache",
    "coverage": "npm t -- --coverage",
    "coverage:open": "open coverage/lcov-report/index.html",
    "test:u": "npm t -- -u",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": "git+https://github.com/architecturejs/aj-site.git",
  "keywords": [
    "js",
    "architecture",
    "component-architecture",
    "component-driven-architecture",
    "js",
    "javascript",
    "frontend",
    "frontend-architecture"
  ],
  "author": "Аневризма",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/architecturejs/aj-site/issues"
  },
  "homepage": "https://github.com/architecturejs/aj-site#readme",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-async-generators": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.5.5",
    "@babel/runtime": "7.5.5",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-loader": "8.0.6",
    "commitizen": "4.0.3",
    "css-loader": "3.2.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.3.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "4.2.0",
    "flow-bin": "0.107.0",
    "html-webpack-plugin": "3.2.0",
    "import-glob-loader": "1.1.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss": "7.0.18",
    "postcss-loader": "3.0.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "semantic-release": "15.13.24",
    "style-loader": "1.0.0",
    "tailwindcss": "1.1.2",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/scripts/"
    ]
  }
}
